<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <ul>
      <% messages.forEach((message) => { %>
        <li><%= message.text %></li>
        <li><%= message.user %></li>
        <li><%= message.added %></li>
      <% }) %>
    </ul>
    <!-- botao para nova mensagem -->
    <button onclick="window.location.href='/new'">Nova mensagem</button>
    
  </body>
</html>

